"ecmascript:",
"",
"var numCircles = 75;",
"var zAxis      = new SFVec3f (0, 0, 1);",
"var materials  = [ ];",
"",
"",
"function initialize ()",
"{",
"\tvar",
"\t\tcurrentScene = Browser .currentScene,",
"\t\tgroup        = currentScene .getNamedNode (\"Circles\");",
"",
"\tgroup .children .length = 0;",
"",
"\tfor (var i = 0; i < numCircles; ++ i)",
"\t{",
"\t\tvar",
"\t\t\ttransform  = currentScene .createNode (\"Transform\"),",
"\t\t\tshape      = currentScene .createNode (\"Shape\"),",
"\t\t\tappearance = currentScene .createNode (\"Appearance\"),",
"\t\t\tmaterial   = currentScene .createNode (\"Material\"),",
"\t\t\tdisk       = currentScene .createNode (\"Disk2D\");",
"",
"\t\tgroup .children [i]     = transform;",
"\t\ttransform .children [0] = shape;",
"\t\tshape .appearance       = appearance;",
"\t\tshape .geometry         = disk;",
"\t\tappearance .material    = material;",
"\t\tmaterial .diffuseColor  = new SFColor ();",
"\t\tmaterial .emissiveColor = new SFColor (1, 1, 1);",
"\t\tmaterial .transparency  = i / (numCircles - 1);",
"",
"\t\tmaterials .push (material);",
"",
"\t\tplace (transform);",
"\t}",
"}",
"",
"function set_hitPoint (value)",
"{",
"\tvalue .z = 0;",
"",
"\tvar group = Browser .currentScene .getNamedNode (\"Circles\");",
"",
"\tfor (var i = 0; i < numCircles; ++ i)",
"\t{",
"\t\tvar",
"\t\t\ttransform = group .children [i],",
"\t\t\tdirection = transform .translation .subtract (value),",
"\t\t\tdistance  = direction .length ();",
"",
"\t\tdirection = direction .normalize () .multiply (1 / (Math .pow (distance * 50, 2)));",
"",
"\t\ttransform .translation = transform .translation .add (direction);",
"\t}",
"}",
"",
"function set_time ()",
"{",
"\tvar group = Browser .currentScene .getNamedNode (\"Circles\");",
"",
"\tfor (var i = 0; i < numCircles; ++ i)",
"\t{",
"\t\tvar",
"\t\t\tdt        = 1 / Browser .currentFrameRate,",
"\t\t\ttransform = group .children [i],",
"\t\t\tmaterial  = transform .children [0] .appearance .material;",
"",
"\t\tmaterial .transparency += dt / numCircles * 4;",
"\t\ttransform .rotation     = new SFRotation (zAxis, material .transparency * Math .PI * 2 * 8);",
"",
"\t\tif (material .transparency > 1)",
"\t\t{",
"\t\t\tmaterial .transparency = dt > 1 ? Math .random (): 0;",
"\t\t\tplace (group .children [Math .round (random (0, numCircles - 1))]);",
"\t\t}",
"\t}",
"}",
"",
"function place (transform)",
"{",
"\tvar appearance = transform .children [0] .appearance;",
"",
"\ttransform .translation .x = random (-2, 2);",
"\ttransform .translation .y = random (-1, 1);",
"\ttransform .rotation       = new SFRotation (zAxis, Math .random () * 2 * Math .PI);",
"\ttransform .scale .x       = random (0.05, 0.2);",
"\ttransform .scale .y       = transform .scale .x;",
"",
"\tappearance .material .emissiveTexture = textures [Math .round (Math .random () * 2)];",
"}",
"",
"function random (min, max)",
"{",
"\treturn Math .random () * (max - min) + min;",
"}"
