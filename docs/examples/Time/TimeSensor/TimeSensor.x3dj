"",
"\tstring_changed [0] = fill (hours24 * 24) + \":\" + fill (minutes * 60) + \":\" + fill (seconds * 60);",
"}",
"",
"function fill (value)",
"{",
"\tvalue = Math .floor (value);",
"",
"\tif (value < 10)",
"\t\treturn \"0\" + value;",
"",
