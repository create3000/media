"ecmascript:",
"",
"function initialize ()",
"{",
"   const ",
"      scene   = Browser .currentScene,",
"      numbers = [ ];",
"",
"   for (let y = 0; y < 4; ++ y)",
"   {",
"      for (let x = 0; x < 4; ++ x)",
"      {",
"         const number = scene .createProto (\"Number\");",
"",
"         Browser .addRoute (proxy, \"orientation_changed\", number, \"set_rotation\");",
"",
"         numbers .push (number);",
"      }",
"   }",
"",
"   for (let z = 0; z < 4; ++ z)",
"   {",
"      for (let y = 0; y < 4; ++ y)",
"      {",
"         for (let x = 0; x < 4; ++ x)",
"         {",
"            const trans = scene .createNode (\"Transform\");",
"",
"            trans .translation .x = x / 2 - 0.75;",
"            trans .translation .y = y / 2 - 0.75;",
"            trans .translation .z = z / 2 - 0.75;",
"",
"            trans .children .push (numbers [Math .floor (Math .random () * 16)]);",
"",
"            group .children .push (trans);",
"         }",
"      }",
"   }",
"}",
""
