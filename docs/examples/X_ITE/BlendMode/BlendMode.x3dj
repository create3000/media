"ecmascript:",
"",
"let up = true;",
"",
"const ",
"   low  = Math .random (),",
"   high = Math .random ();",
"",
"function set_fraction (value)",
"{",
"   const v = low + value * (high - low);",
"   const s = v .toFixed (1);",
"",
"   if (s === text .string [0])",
"      return;",
"",
"   text .string [0] = s;",
"}",
